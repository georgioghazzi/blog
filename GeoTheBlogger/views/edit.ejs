<% include partials/header %>

<div class="container form">
<div class=jumbotron>
<h1>Edit Your Profile</h1>
</div>
<form action="/edit" action="upload" method="POST">	
	<div class="form-group">
    <input class="form-control"  name="name" placeholder="Enter Your Full Name" type="text">
    </div>
    <div class="form-group">
    <input class="form-control"  name="work" placeholder="Enter Your Work Description" type="text">
    </div>
    <div class="form-group">
    <textarea class="form-control" rows="5" id="description" name="description" placeholder="Enter A Description About You"></textarea>
    </div>
    <div class="form-group">
	  <span class="btn btn-primary btn-file">
	    Browse <input type="file" name="filename">
	</span>
	</div>
    <div class="form-group">
    <button class="btn btn-primary">Edit Profile</button>
    </div>
</form>




<% include partials/footer %>